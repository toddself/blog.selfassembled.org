<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{title}}</title>
  <link rel="alternate" type="text/html" href="{{site_url}}"/>
  <link rel="self" type="application/atom+xml" href="{{site_url}}/index.rss"/>
  <id>tag:{{domain}},{{tag_date}}:index.rss</id>
  <updated>{{time_stamp}}</updated>
  <subtitle>
  {{description}}
  </subtitle>
  <generator uri="https://github.com/toddself/blog-builder">Blog Builder</generator>
  {{#each entries}}
  <entry>
  <title type="html">
    {{this.title}}
  </title>
  <link rel="alternate" type="text/html" href="{{this.site_url}}/{{this.url}}"/>
  <id>tag:{{this.domain}},{{this.tag_date}}:{{this.url}}</id>
  <published>{{this.modified}}</published>
  <updated>{{this.modified}}</updated>
  <author>
  <name>{{this.author}}</name>
  <uri>{{this.site_url}}</uri>
  </author>
  <content type="html" xml:lang="en" xml:base="{{this.site_url}}">
    <![CDATA[{{{this.contents}}}]]>
  </content>
  </entry>
{{/each}}
</feed>
